@{
}

<h1> Index </h1>

@*Show total data*@
<h3> Total Data: @ViewData["TotalData"] </h3>

<ul>

    @foreach (var user in ViewData["user"] as List<UserModel>)
    {
        <li> ID: @user.Id </li>  
        <li> Username: @user.Username </li>  
        <li> Password: @user.Password </li>  
    }

</ul>